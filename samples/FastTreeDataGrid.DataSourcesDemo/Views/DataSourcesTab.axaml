                      <WidgetTemplate>
                        <FormattedTextWidget EmSize="13" />
                      </WidgetTemplate>
                    </FastTreeDataGridColumn.CellTemplate>
                  </FastTreeDataGridColumn>
                  <FastTreeDataGridColumn Header="Details"
                                          SizingMode="Star"
                                          MinWidth="320"
