                                RowHeight="36"
                                IndentWidth="18"
                                HeaderHeight="36">
                <FastTreeDataGrid.Columns>
                  <FastTreeDataGridColumn Header="Widget"
                                          SizingMode="Star"
                                          MinWidth="260"
                                          IsHierarchy="True"
                                          ValueKey="{x:Static widgets:WidgetGalleryNode.KeyName}">
