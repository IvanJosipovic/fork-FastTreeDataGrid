    <TabItem Header="Widget Metrics">
      <Grid DataContext="{Binding DataSources}">
        <TabControl>
          <TabItem Header="Async">
            <Border BorderBrush="#CCC" BorderThickness="1" CornerRadius="4" Padding="0" Margin="0,8,0,0">
